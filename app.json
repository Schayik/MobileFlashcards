{
  "expo": {
    "name": "mobile-flashcards",
    "description": "easy way to train yourself to memorize things",
    "slug": "mobile-flashcards",
    "version": "1.0",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/psv.png",
    "notification": {
      "icon": "./assets/psv.png",
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mobile-flashcards"
    },
    "android": {
      "package": "mobile-flashcards"
    }
  }
}
